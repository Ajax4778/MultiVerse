<!DOCTYPE html>
<html>
<head>
  <title>MultiVerse</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if flash.now[:msg] %>
    <ul>
      <% flash.now[:msg].each do |m| %>
        <li><%= m %></li>
      <% end %>
    </ul>
  <% end %>

<%= yield %>

</body>
</html>
